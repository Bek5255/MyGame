<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    tools:context=".HelpActivity">

    <RelativeLayout
        android:layout_width="210dp"
        android:layout_height="210dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RelativeLayout
            android:id="@+id/relativeLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="1dp"
            android:layout_marginEnd="1dp"
            android:layout_marginBottom="1dp">

            <ImageButton
                android:id="@+id/btn_0_0"
                style="@style/btn_style"
                tools:ignore="TouchTargetSizeCheck" />

            <ImageButton
                android:id="@+id/btn_0_1"
                style="@style/btn_style"
                android:layout_toEndOf="@+id/btn_0_0"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

            <ImageButton
                android:id="@+id/btn_0_2"
                style="@style/btn_style"
                android:layout_toEndOf="@+id/btn_0_1" />

            <ImageButton
                android:id="@+id/btn_0_3"
                style="@style/btn_style"
                android:layout_toEndOf="@+id/btn_0_2" />

            <ImageButton
                android:id="@+id/btn_0_4"
                style="@style/btn_style"
                android:layout_toEndOf="@+id/btn_0_3" />

            <ImageButton
                android:id="@+id/btn_1_0"
                style="@style/btn_style"
                android:layout_below="@id/btn_0_0" />

            <ImageButton
                android:id="@+id/btn_1_1"
                style="@style/btn_style"
                android:layout_below="@id/btn_0_0"
                android:layout_toEndOf="@+id/btn_1_0" />

            <ImageButton
                android:id="@+id/btn_1_2"
                style="@style/btn_style"
                android:layout_below="@id/btn_0_0"
                android:layout_toRightOf="@+id/btn_1_1" />

            <ImageButton
                android:id="@+id/btn_1_3"
                style="@style/btn_style"
                android:layout_below="@id/btn_0_0"
                android:layout_toEndOf="@+id/btn_1_2" />

            <ImageButton
                android:id="@+id/btn_1_4"
                style="@style/btn_style"
                android:layout_below="@id/btn_0_0"
                android:layout_toEndOf="@+id/btn_1_3" />

            <ImageButton
                android:id="@+id/btn_2_0"
                style="@style/btn_style"
                android:layout_below="@id/btn_1_0" />

            <ImageButton
                android:id="@+id/btn_2_1"
                style="@style/btn_style"
                android:layout_below="@id/btn_1_0"
                android:layout_toEndOf="@+id/btn_1_0"
                tools:ignore="TouchTargetSizeCheck" />

            <ImageButton
                android:id="@+id/btn_2_2"
                style="@style/btn_style"
                android:layout_below="@id/btn_1_0"
                android:layout_toEndOf="@+id/btn_1_1" />

            <ImageButton
                android:id="@+id/btn_2_3"
                style="@style/btn_style"
                android:layout_below="@id/btn_1_0"
                android:layout_toEndOf="@+id/btn_1_2" />

            <ImageButton
                android:id="@+id/btn_2_4"
                style="@style/btn_style"
                android:layout_below="@id/btn_1_0"
                android:layout_toEndOf="@+id/btn_1_3" />

            <ImageButton
                android:id="@+id/btn_3_0"
                style="@style/btn_style"
                android:layout_below="@id/btn_2_0" />

            <ImageButton
                android:id="@+id/btn_3_1"
                style="@style/btn_style"
                android:layout_below="@id/btn_2_0"
                android:layout_toEndOf="@+id/btn_1_0" />

            <ImageButton
                android:id="@+id/btn_3_2"
                style="@style/btn_style"
                android:layout_below="@id/btn_2_0"
                android:layout_toEndOf="@+id/btn_1_1" />

            <ImageButton
                android:id="@+id/btn_3_3"
                style="@style/btn_style"
                android:layout_below="@id/btn_2_0"
                android:layout_toEndOf="@+id/btn_1_2" />

            <ImageButton
                android:id="@+id/btn_3_4"
                style="@style/btn_style"
                android:layout_below="@id/btn_2_0"
                android:layout_toEndOf="@+id/btn_1_3" />

            <ImageButton
                android:id="@+id/btn_4_0"
                style="@style/btn_style"
                android:layout_below="@id/btn_3_0" />

            <ImageButton
                android:id="@+id/btn_4_1"
                style="@style/btn_style"
                android:layout_below="@id/btn_3_0"
                android:layout_toEndOf="@+id/btn_1_0" />

            <ImageButton
                android:id="@+id/btn_4_2"
                style="@style/btn_style"
                android:layout_below="@id/btn_3_0"
                android:layout_toEndOf="@+id/btn_1_1" />

            <ImageButton
                android:id="@+id/btn_4_3"
                style="@style/btn_style"
                android:layout_below="@id/btn_3_0"
                android:layout_toEndOf="@+id/btn_1_2" />

            <ImageButton
                android:id="@+id/btn_4_4"
                style="@style/btn_style"
                android:layout_below="@id/btn_3_0"
                android:layout_toEndOf="@+id/btn_1_3" />

        </RelativeLayout>
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>